{
  "extends": [
    "eslint:recommended", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes": 0,
    "no-console": 0,
    "no-debugger": 0,
    "no-unused-vars": 0,
    "no-case-declarations" : 0,
    "react/display-name": 0,
    "react/prop-types": 0,
    "react/no-multi-comp": 0
  },
  "globals": {
        "DB_MODELS": true,
        "GAME_QUEUE": false,
        "DB_CREDENTIALS": true,
        "GlobalConstant": true,
        "PROMISE": true,
        "_": true,
        "async": true,
        "SOCKET_IO": true,
        "POKER_QUEUE": true
    }
}
