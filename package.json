{
  "name": "poker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev:start": "export NODE_ENV=development; PORT=3100 nodemon ./bin/www",
    "webserver": "webpack-dev-server --port 3000 --progress --colors --inline --hot --host 0.0.0.0 --content-base build/",
    "dev": "concurrently 'npm run dev:start' 'npm run webserver'",
    "debug": "export NODE_ENV=development; PORT=3100 node --inspect --debug-brk ./bin/www",
    "stats": "webpack --profile --json > stats.json",
    "start:webserver": "webpack-dev-server",
    "deploy": "gh-pages -d build",
    "build": "rm -rf build/* && webpack -p",
    "test": "karma start",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint ./app ./tests --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache"
  },
